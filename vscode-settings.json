{
  "terminal.external.osxExec": "iTerm.app",
  "editor.renderWhitespace": "none",
  "files.insertFinalNewline": true,
  "editor.fontFamily": "Fira Code",
  "editor.matchBrackets": false,
  "editor.scrollBeyondLastLine": true,
  "editor.tabSize": 2,
  "editor.rulers": [100],
  "files.trimTrailingWhitespace": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.snippetSuggestions": "top",
  "workbench.colorTheme": "Material Theme High Contrast",
  "workbench.iconTheme": "eq-material-theme-icons",
  "git.autofetch": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "workbench.statusBar.feedback.visible": false,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
      "activityBarBadge.background": "#80CBC4",
      "list.activeSelectionForeground": "#80CBC4",
      "list.inactiveSelectionForeground": "#80CBC4",
      "list.highlightForeground": "#80CBC4",
      "scrollbarSlider.activeBackground": "#80CBC450",
      "editorSuggestWidget.highlightForeground": "#80CBC4",
      "textLink.foreground": "#80CBC4",
      "progressBar.background": "#80CBC4",
      "pickerGroup.foreground": "#80CBC4",
      "tab.activeBorder": "#80CBC4",
      "notificationLink.foreground": "#80CBC4",
      "editorWidget.resizeBorder": "#80CBC4",
      "editorWidget.border": "#80CBC4",
      "settings.modifiedItemIndicator": "#80CBC4",
      "settings.headerForeground": "#80CBC4",
      "panelTitle.activeBorder": "#80CBC4",
      "breadcrumb.activeSelectionForeground": "#80CBC4",
      "menu.selectionForeground": "#80CBC4",
      "menubar.selectionForeground": "#80CBC4",
      "statusBar.debuggingBackground": "#0876A2",
      "terminal.ansiGreen": "#00A681"
  },
  "materialTheme.autoApplyIcons": true,
  "terminal.integrated.shellArgs.osx": [
      "-l"
  ],
  "ngComponents.angularRoot": "js/",
  "ngComponents.memberDiagnostics.enabled": true,
  "npm.packageManager": "yarn",
  "ngComponents.directiveGlobs": [
      "**/*.js"
  ],
  "ngComponents.controllerGlobs": [
      "**/*.js"
  ],
  "ngComponents.componentGlobs": [
      "**/*.js"
  ],
  "ngComponents.forceEnable": true,
  "material-icon-theme.folders.color": "#0876a2",
  "material-icon-theme.opacity": 0.8,
  "material-icon-theme.hidesExplorerArrows": true,
  "materialTheme.accent": "Teal",
  "editor.fontLigatures": true,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.scopes": [
      "document",
      "containers"
  ],
  "dateTime.use24HourClock": false,
  "dateTime.padHours": false,
  "dateTime.showAMPM": true,
  "css.lint.duplicateProperties": "warning",
  "css.lint.idSelector": "warning",
  "css.lint.zeroUnits": "warning",
  "scss.scanImportedFilesDepth": 150,
  "gitlens.heatmap.toggleMode": "window",
  "diffEditor.renderSideBySide": true,
  "debug.toolBarLocation": "hidden",
  "debug.showInStatusBar": "never",
  "statusbarDebugger.priority": 100,
  "gitlens.codeLens.enabled": false,
  "files.associations": {
      "*.geojson": "json"
  },
  "gitlens.views.repositories.location": "scm",
  "gitlens.views.fileHistory.location": "scm",
  "gitlens.views.lineHistory.location": "scm",
  "gitlens.views.search.location": "scm",
  "gitlens.views.compare.location": "scm",
  "gitlens.mode.active": "review",
  "breadcrumbs.enabled": true,
  "breadcrumbs.symbolPath": "off",
  "workbench.editor.closeEmptyGroups": false,
  "window.titleBarStyle": "native",
  "window.title": "${dirty}${activeEditorMedium}${separator}${rootName}",
  "editor.minimap.enabled": false,
  "gitlens.views.repositories.files.layout": "tree",
  "gitlens.views.compare.files.layout": "list",
  "git.fetchOnPull": true,
  "git.ignoreLimitWarning": true,
  "vscodeWorkspaceSwitcher.paths": [
      "/Users/keshleman/dev/vscode-workspaces"
  ],
  "window.nativeTabs": true,
  "gitlens.settings.mode": "advanced"
}
